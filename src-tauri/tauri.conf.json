{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "radiantgens-changelog-generator",
  "version": "0.1.0",
  "identifier": "com.rohandacoder.radiantgens-changelog-generator",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "RadiantGens Changelog Generator",
        "width": 1200,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.icns", "icons/icon.ico"]
  }
}
